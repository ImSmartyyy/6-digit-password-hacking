int currentIndex = 0;
int count = 0;

void setup() {
  DigiKeyboard.update();
  DigiKeyboard.sendKeyStroke(0);
  delay(3000);
}

void loop() {
  // Write 5 lines
  for (int i = 0; i < 5 && currentIndex < sizeof(customText) / sizeof(customText[0]); ++i) {
    DigiKeyboard.print(customText[currentIndex]);
    DigiKeyboard.sendKeyStroke(100); // Press Enter key after each line
    currentIndex++;
  }
  
  count++;
  
  if (count == 1) {
    triggerRetry();
    count = 0;
  }
}

void triggerRetry() {
  digitalWrite(1, HIGH);
  DigiKeyboard.sendKeyStroke(40);
  delay(60000); // Wait for 60 seconds
  digitalWrite(1, LOW);
}
